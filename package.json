{
  "name": "app",
  "version": "1.0.0",
  "main": "n/a",
  "repository": "mcansh/next-boilerplate",
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh)",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4000",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint ./ --ext .js,.ts,.tsx",
    "lint:css": "stylelint './**/*.js' './**/*.jsx' './**/*.tsx' './**/*.ts'",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|graphql|mdx)\"",
    "test": "jest",
    "validate": "npm-run-all --parallel lint lint:css type-check test",
    "analyze": "ANALYZE=true yarn build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "sh scripts/ts-precommit.bash",
      "yarn lint:css",
      "git add"
    ]
  },
  "dependencies": {
    "@mcansh/next-now-base-url": "1.2.0",
    "@mcansh/next-nprogress": "1.1.2",
    "@next/bundle-analyzer": "9.3.0",
    "@sentry/browser": "5.14.2",
    "@sentry/node": "5.14.2",
    "@zeit/next-source-maps": "0.0.4-canary.1",
    "babel-plugin-styled-components": "1.10.7",
    "got": "10.6.0",
    "he": "1.2.0",
    "jest": "25.1.0",
    "ky": "0.19.0",
    "ky-universal": "0.5.0",
    "next": "9.3.0",
    "next-offline": "5.0.0",
    "next-server": "9.0.5",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-ink": "6.4.0",
    "react-simple-img": "2.3.9",
    "styled-components": "5.0.1",
    "type-fest": "0.12.0"
  },
  "devDependencies": {
    "@mcansh/eslint-config": "2.6.1",
    "@testing-library/jest-dom": "5.1.1",
    "@testing-library/react": "10.0.1",
    "@types/got": "9.6.9",
    "@types/he": "1.1.1",
    "@types/jest": "25.1.4",
    "@types/node": "13.9.1",
    "@types/react": "16.9.23",
    "@types/react-dom": "16.9.5",
    "@types/styled-components": "5.0.1",
    "@typescript-eslint/eslint-plugin": "2.24.0",
    "@typescript-eslint/parser": "2.24.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-kentcdodds": "14.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-you-dont-need-lodash-underscore": "6.10.0",
    "husky": "4.2.3",
    "lint-staged": "10.0.8",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "stylelint": "13.2.1",
    "stylelint-config-prettier": "8.0.1",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-prettier": "1.1.2",
    "stylelint-processor-styled-components": "1.10.0",
    "typescript": "3.8.3"
  },
  "resolutions": {
    "js-yaml": ">=3.13.1 >=3.13.0",
    "set-value": ">=2.0.1 <3.0.0 || >=3.0.1"
  }
}
